{
  "name": "receipt-tracker-agent",
  "version": "1.0.0",
  "description": "Intelligent receipt and e-wallet transaction tracking agent using Telegram, LangChain, and Supabase",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "gcp:deploy": "cd gcp-free-tier && ./deploy-with-env.sh",
    "gcp:start": "cd gcp-free-tier && ./manage-instance.sh start",
    "gcp:stop": "cd gcp-free-tier && ./manage-instance.sh stop",
    "gcp:restart": "cd gcp-free-tier && ./manage-instance.sh restart",
    "gcp:status": "cd gcp-free-tier && ./manage-instance.sh status",
    "gcp:ssh": "cd gcp-free-tier && ./manage-instance.sh ssh",
    "gcp:info": "cd gcp-free-tier && ./manage-instance.sh info",
    "gcp:logs": "gcloud compute ssh free-tier-vm --zone=us-central1-a --command='pm2 logs'",
    "gcp:pm2-status": "gcloud compute ssh free-tier-vm --zone=us-central1-a --command='pm2 status'",
    "gcp:pm2-restart": "gcloud compute ssh free-tier-vm --zone=us-central1-a --command='cd ~/receipt-tracker && pm2 restart receipt-tracker-agent'"
  },
  "keywords": [
    "telegram",
    "receipt",
    "tracker",
    "langchain",
    "langgraph",
    "supabase"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@langchain/core": "^0.3.0",
    "@langchain/langgraph": "^0.2.0",
    "@langchain/openai": "^0.3.0",
    "@supabase/supabase-js": "^2.39.0",
    "dotenv": "^16.3.1",
    "langchain": "^0.3.0",
    "sharp": "^0.33.0",
    "telegraf": "^4.15.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  }
}
